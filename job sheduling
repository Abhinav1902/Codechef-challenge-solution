#include<bits/stdc++.h>
struct meet{
	int deadline;
	int profit;
};
bool comparator(struct meet m1,meet m2){
    return m1.profit>m2.profit;
}
int jobScheduling(vector<vector<int>> &jobs){
   int n=jobs.size();
	struct meet job[n];
	for(int i=0;i<n;i++){
		job[i].deadline=jobs[i][0];
		job[i].profit=jobs[i][1];
	}
	sort(job,job+n,comparator);
	int maxi=0;
	for(int i=0;i<n;i++){
		maxi=max(maxi,job[i].deadline);
	}
	vector<int>store(maxi+1,-1);
	int totalprofit=0;
	for(int i=0;i<n;i++){
		for(int j=job[i].deadline;j>0;j--){
			if(store[j]==-1){
				store[j]=0;
				totalprofit+=job[i].profit;
				break;
				
			}
		}
	}
	return totalprofit;
}
